<!--Cabecero usando Bootstrap-->
<!--indicamos que es el cabecero principal y un padding del 2-->
<!--aplicamos el estilo de background info, osea el estilo info para el background-->
<!--y el texto estara en blanco-->
<header id="main-header" class="py-2 bg-info text-white">
    <!--por el hecho de usar container el titulo se recorre un poco a la derecha-->
    <div class="container">
        <!--indicamos que esta columna es md (para mediano) 6 columnas-->
        <div class="col-mid-6">
            <!--esta etiqueta de i es para agregar un icono-->
            <!-- esto es para poner el icono de engrane 'fas fa-cog' -->
            <h1> <i class="fas fa-cog"></i>  Control de Clientes</h1>
        </div>
    </div>
</header>